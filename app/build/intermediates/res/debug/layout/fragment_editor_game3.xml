<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.beebzb.bakalarka.EditorGame3Fragment">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:gravity="center_vertical|center_horizontal">

        <LinearLayout
            android:id="@+id/leftLinesContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/L1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/leftMouseImg"
                    style="@style/EditorAnimalIcon"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/mouse" />

                <view
                    android:id="@+id/npMouse"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/leftCatImg"
                    style="@style/EditorAnimalIconRightInRow"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/cat" />

                <view
                    android:id="@+id/npCat"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/L2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/leftGooseImg"
                    style="@style/EditorAnimalIcon"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/goose" />

                <view
                    android:id="@+id/npGoose"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/leftDogImg"
                    style="@style/EditorAnimalIconRightInRow"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/dog" />

                <view
                    android:id="@+id/npDog"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/L3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/leftGoatImg"
                    style="@style/EditorAnimalIcon"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/goat" />

                <view
                    android:id="@+id/npGoat"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/leftRamImg"
                    style="@style/EditorAnimalIconRightInRow"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/ram" />

                <view
                    android:id="@+id/npRam"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/L4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/CowImg"
                    style="@style/EditorAnimalIcon"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/cow" />

                <view
                    android:id="@+id/npCow"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/npHorseImg"
                    style="@style/EditorAnimalIconRightInRow"
                    android:layout_width="@dimen/editor_fragment_radius_icons"
                    android:layout_height="@dimen/editor_fragment_radius_icons"
                    android:src="@drawable/horse" />

                <view
                    android:id="@+id/npHorse"
                    class="com.example.beebzb.bakalarka.layout.MyNumberPicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />
            </LinearLayout>


        </LinearLayout>

        <Button style="@style/CustomDialogButton"
            android:layout_width="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_height="wrap_content"
            android:text="@string/editor_button_23"
            android:id="@+id/hasSolutionButton"
            android:layout_gravity="center_horizontal" />
    </LinearLayout>

</LinearLayout>
